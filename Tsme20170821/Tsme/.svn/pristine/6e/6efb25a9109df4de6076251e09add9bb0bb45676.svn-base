div{
	font-family:Microsoft Yahei;
	font-size:14px;
}
div,ul,li,span,input{
	margin:0;
	padding:0;
}
ul,li{
	list-style:none;
}
.title{
	width:100%;
	height:15%;
	background-color:#5da8e4;
}

.title h1{
	text-align:center;
	line-height:100px;
	font-family:Microsoft YaHei;
	font-size:24px;
	color:#fcfcfc;	
}
.wrap{
	border: 1px dotted gray;
	min-width: 1100px;
	max-width: 1200px;
	margin:auto;
}
.navigator{
	float: left;
	width: 200px;
    border: 1px solid #aed0ea;
    margin-top: 10px;
    height:700px;
}
.navigator li{
	height:30px;
	line-height:30px;
	text-align: center;
    font-size: 17px;
    background-color: #ccc;
    margin-bottom:15px;	
}
.navigator li a{
	cursor:pointer;
}
.navigator li a:hover{
	color:blue;
}
.rightDiv{
	float:right;
	width: calc(100% - 202px);
}
.infoTitle{
    width: 100%;
    border: 1px solid red;
    display: block;
    text-align: center;
    background-color: #ccc;
    font-size: 24px;
    font-family: ºÚÌå;
    color: #fff;
}
.userLi {
	border:1px solid black;
}
.userLi span{
    display: inline-block;
    margin: 0px;
    width: 24%;
    text-align: center;
    font-size: 16px;
    border-left:1px solid black;
}
.addLi{
	position:relative;
}
.addLi, .addLi span{
	height:25px;
}
.addLi span label{
    float: left;
    margin-left: 10px;
}
.addLi .last{
	position:absolute;
}

.maskDiv{
	width:100%;
	height:100%;
	position:absolute;
	background-color:#ccc;
	opacity:0.5;
	top:0px;
}
.conDiv{
	display:none;
    width: 600px;
    height: 300px;
    position: absolute;
    top: calc(50% - 150px);
    left: calc(50% - 300px);
    opacity: 1;
    background-color: #fff;
    border: 1px solid #888;
    border-radius: 11px 11px 0px 0px;
}
.addUserDiv{
	display:none;
}
.addTitle{
	height:50px;
	line-height:50px;
	font-size:20px;
	text-align:center;
	display:block;
    background-color: #aaa;
    color: #fff;
    border-radius: 10px 10px 0px 0px;
}
.addUserDiv ul li{
	height:50px;
	line-height:50px;
	margin-left:calc(50% - 150px);
}
.addUserDiv ul li span{
	width: 75px;
    display: inline-block;
}
.addUserDiv ul li>input{
	height:20px;
}
.addUserDiv ul li label{
    cursor: pointer;
    width: 50px;
    display: inline-block;
}
label input{
	cursor:pointer;
}
.addUserDiv .save{
    height: 30px;
    width: 87px;
    line-height: 30px;
    text-align: center;
    font-size: 20px;
    margin-left: 100px;
    background-color: #888;
    display: block;
    border-radius: 10px;
    cursor: pointer;
    color:#fff;
}
.addUserDiv .save:hover{
	background-color:#ccc;
	color:#000;
}